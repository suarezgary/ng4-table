{
  "name": "ng4-table",
  "version": "1.0.0",
  "description": "A Angular 4 module to create dinamic tables",
  "main": "dist/index.js",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ng4-table.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng4-table.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "keywords": [
    "ng4",
    "angular",
    "4",
    "table",
    "ng4-table"
  ],
  "author": "Gary Suarez",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^4.2.5",
    "@angular/compiler": "^4.2.5",
    "@angular/compiler-cli": "^4.2.5",
    "@angular/core": "^4.2.5",
    "@angular/forms": "^4.2.5",
    "rollup": "^0.43.0",
    "rxjs": "^5.1.0",
    "typescript": "^2.4.1",
    "uglify-js": "^3.0.23"
  },
  "dependencies": {}
}
